{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "indent": [
      true,
      "spaces",
      2
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "radix": true,
    "interface-name": false,
    "eofline": false,
    "align": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "no-namespace": false,
    "no-empty-interface": false,
    "no-console": false,
    "no-bitwise": false,
    "max-classes-per-file": false,
    "ordered-imports": false,
    "array-type": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export | implements "
      }
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/*.generated.*"
    ]
  }
}